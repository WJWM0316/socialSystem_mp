.header{
  box-sizing: border-box;
  padding-top: 185rpx;
  position: relative;
  width: 100%;
  height: 482rpx;
}
.headerBg{
  width: 100%;
  position: absolute;
  z-index: -1;
  top: 0;
  left: 0;
}
.header .title{
  text-align: center;
  display: block;
  font-size:56rpx;
  line-height: 56rpx;
  font-weight:600;
  color: #FFFFFF;
}
.header .subtitle{
  text-align: center;
  display: block;
  margin-top: 17rpx;
  font-size:26rpx;
  line-height: 28rpx;
  font-weight:400;
  color: #FFF;
}
.commont{
  border-radius: 50rpx;
  height: 88rpx;
  width: 550rpx;
  background-color: #E8E9EB;
  text-align: center;
  margin: 0 auto;
  margin-top: 30rpx;
  font-size: 32rpx;
}
.commont input{
  box-sizing: border-box;
  padding: 0 20rpx;
  width: 550rpx;
  height: 88rpx;
  text-align: center;
  font-size: 32rpx;
  font-weight:500;
  color: #282828;
  background:rgba(247,249,252,1);
  border-radius: 50rpx;
  border:1rpx solid rgba(232,233,235,1);
}
.timeBox{
  color: #626262;
  margin-top: 30rpx;
  text-align: center;
}
.startTime,
.endTime{
  width: 240rpx;
  height: 88rpx;
  background:rgba(247,249,252,1);
  border-radius: 50rpx;
  display: inline-block;
  border-radius: 50rpx;
  text-align: center;
  line-height: 88rpx;
  font-size: 32rpx;
  font-weight: 500;
  border:1rpx solid rgba(232,233,235,1);
}
.degree{
  color: #282828;
  width: 550rpx;
  height: 80rpx;
  background:rgba(247,249,252,1);
  border-radius: 50rpx;
  margin: 0 auto;
  margin-top: 30rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 32rpx;
  font-weight: 500;
  border:1rpx solid rgba(232,233,235,1);
}
.btn-area {
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 56rpx;
}
.btn-area button{
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;
  border: none;
  outline: none;
  line-height: 128rpx;
  text-align: center;
  box-shadow:0px 20px 18px 0px rgba(255,220,41,0.2);
  background-color: #FFDC29;
}
.btn-area button .icon {
  font-size: 40rpx;
  color: #652791;
}