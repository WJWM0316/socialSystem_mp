
.indexEdit {
	min-height: 100vh;
	background: #FAFCFF;
	padding: 24rpx 30rpx;
}
.head {
	background:rgba(255,255,255,1);
	box-shadow:0rpx 10rpx 30rpx 0rpx rgba(22,39,77,0.05);
	border-radius:10rpx;
	padding: 40rpx 28rpx;
}
.head .msg {
	margin-top: 30rpx;
	padding: 38rpx 0 0;
	border-top: 1rpx dashed #BACED8;
}
.head .msg .item {
	overflow: hidden;
	font-size:28rpx;
	color:rgba(40,40,40,1);
	line-height:28rpx;
	margin-bottom: 32rpx;
}
.head .msg .item:last-child {
	margin-bottom: 0;
}
.head .msg .item .txt {
	float: left;
	color: #626262;
	font-weight: 300;
}
.head .msg .item .value {
	width: 400rpx;
	float: right;
	text-align: right;
}
.peoples, .mainPage, .team {
	padding: 30rpx 70rpx 30rpx 28rpx;
	background:rgba(255,255,255,1);
	box-shadow:0rpx 10rpx 30rpx 0rpx rgba(22,39,77,0.05);
	border-radius:10rpx;
	margin-top: 24rpx;
	overflow: hidden;
	position: relative;
}
.peoples .txt, .mainPage .txt, .team .txt {
	font-size:28rpx;
	font-weight:400;
	color:rgba(40,40,40,1);
	line-height: 60rpx;
	float: left;
}
.boxs {
	display: inline-block;
	font-size: 0;
	float: right;
}
.boxs .people {
	width: 60rpx;
	height: 60rpx;
	margin-right: -12rpx;
	border-radius: 50%;
}
.boxs .people:last-child {
	margin-right: 0;
}
.icon {
	position: absolute;
	font-size: 32rpx;
	color: #CED7DC;
	top: 50%;
	transform: translateY(-50%);
	right: 30rpx;
}
