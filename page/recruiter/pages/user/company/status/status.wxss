.header .line-box {
	font-size:28rpx;
	font-weight:500;
	color:rgba(101,39,145,1);
	line-height: 1;
	position: relative;
}

.header .line-left {
	position: absolute;
	left: 0;
	width: calc(50% - 112rpx);
	top: 50%;
	height: 1px;
	background: #652791;
}

.header .line-left::before {
	content: '';
	display: block;
	width: 15rpx;
	height: 15rpx;
	background: #652791;
	transform: rotate(45deg) translateY(-50%);
	position: absolute;
	right: 0;
	top: 45%;
}

.header .line-right {
	position: absolute;
	right: 0;
	width: calc(50% - 112rpx);
	top: 50%;
	height: 1px;
	background: #652791;
}

.header .line-right::before {
	content: '';
	display: block;
	width: 15rpx;
	height: 15rpx;
	background: #652791;
	transform: rotate(45deg) translateY(-50%);
	position: absolute;
	left: -15rpx;
	top: 45%;
}

.header .button-box {
