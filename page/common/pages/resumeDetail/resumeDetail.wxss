}
.personalTip .text{
  font-size: 28rpx;
  line-height: 1.5;
  font-weight: 300;
  color: #282828;
}
.personalTip .tipBox{
  margin-top: 30rpx;
  width: 100%;
  font-size: 26rpx;
  font-weight: 400;
  color: #652791;
}
.personalTip .tipBox .tip{
  display: inline-block;
  border: 1rpx solid #CEC5DF;
  border-radius: 30rpx;
  padding: 0 30rpx;
  margin-right: 16rpx;
  margin-top: 16rpx;
  height: 48rpx;
  line-height: 48rpx;
  box-sizing:border-box;
}
.personalTip .text{
}
/*意向*/
.intent .officeTab{
  margin-top: 40rpx;
  color: #282828;
  font-size: 32rpx;
  font-weight: 500;
  padding-bottom: 50rpx;
  border-bottom: 1rpx dashed #CED7DC;
}
.intent .officeTab:last-child{
  border-bottom: none;
  padding-bottom: 0rpx;
}
.intent .officeTab .officeDetail{
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: center;
}
.intent .officeTab .officeDetail .name{
  width: 300rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.intent .officeTab .salary{
  color: #FF7F4C;
}
.intent .officeTab .officeInfo{
  font-size: 28rpx;
  font-weight: 300;
  margin-top: 5rpx;
}
/*工作经历*/
.workExp{}
.workExp .moreExp{
  font-size: 26rpx;
  color: #652791;
  font-weight: 300;
  margin-top: 50rpx;
}
/*更多*/
.more .moreTitle {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.more .expContent{
  color: #282828 !important;
}

.more .imgBox{
  margin-top: 20rpx;
}
.more .imgBox::after{
  content: '';
  display: block;
  height: 0;
  clear: both;
}
.more .imgBox image{
  float: left;
  width: 146rpx;
  height: 146rpx;
  background-color: #ccc;
  margin-right: 8rpx;
  margin-bottom: 8rpx;
  border-radius: 2rpx;
}
.imgBox image:nth-of-type(4n){
  margin-right: 0;
}
/*开撩约面*/
.bottomBox{
  position: relative;
  background-color: #F5F4F7;
}
.collect{
  display: block;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  bottom: 162rpx;
  right: 20rpx;
  background-color: #FFFFFF;
  color: #8452A7;
  box-shadow:0px 4px 14px 0px rgba(101,39,145,0.18);
}
.communicate{
  height: 138rpx;
  padding: 20rpx;
  width: 100%;
  border-radius: 28rpx 28rpx 0 0;
  background-color: #FFFFFF;
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  box-sizing: border-box;
}
.communicate .hrAvatar{
  width: 100rpx;
  height: 100rpx;
  flex-shrink: 0;
  background-color: #ccc;
  margin-right: 12rpx;
  border-radius: 50%;
}
.communicate text{
  font-size: 22rpx;
  color: #282828;
  font-weight: 300;
}
.communicate .name{
  font-size: 32rpx;
  font-weight: 500;
  display: block;
}
.communicate .btn{
  float: right;
  width: 250rpx;
  height: 80rpx;
  background-color: #652791;
  color: #FFFFFF;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 40rpx;
  margin-left: 34rpx;
}

/*公共样式*/
.commentBox{
  padding: 40rpx 40rpx 50rpx;
  width: 100%;
  background-color: #FFFFFF;
  box-sizing: border-box;
  border-radius: 10rpx;
  box-shadow:0px 4px 20px 0px rgba(101,39,145,0.1);
  margin-bottom: 20rpx;
}
.commentBox .officeTab:last-of-type,
.commentBox .listCareers .expTab:last-of-type{
  padding-bottom: 0;
  border-bottom: none;
}
.commentBox .add{
  display: block;
  width: 610rpx;
  height: 80rpx;
  border: 1rpx solid #652791;
  border-radius: 42rpx;
  color: #652791;
  font-size: 28rpx;
  font-weight: 400;
  text-align: center;
  line-height: 80rpx;
  margin-top: 60rpx;
}
.commontIcon{
  margin-right: 16rpx;
  display: inline-block;
  width: 24rpx;
  height: 28rpx;
  position: relative;
}
.commontIcon .box{
  display: block;
  width: 24rpx;
  height: 28rpx;
  border: 2rpx solid #652791;
  box-sizing: border-box;
  position: absolute;
  top: 4rpx;
  left: 4rpx;
}
.commontIcon .box::after{
  content: '';
  display: block;
  width: 24rpx;
  height: 28rpx;
  position: absolute;
  top: -6rpx;
  left: -5rpx;
}
.commontIcon::before{
  content: '';
  display: block;
  width: 24rpx;
  height: 28rpx;
  background-color: #EFE9F4;
  position: absolute;
  top: -2rpx;
  left: -1rpx;
  z-index: 0;
}
.intentTitle{
  font-size: 28rpx;
  color: #626262;
  font-weight: 500;
}

.expTab{
  color: #282828;
  padding: 40rpx 0 50rpx;
  border-bottom: 1px dashed #CED7DC;
}
.expTab:last-child {
  border: none;
  padding-bottom: 0;
}
.expTab .expTitle{
  display: flex;
  justify-content: space-between;
}
.expTab .expName{
  display: inline-block;
  min-width: 349rpx;
  font-size: 32rpx;
  font-weight: 500;
  line-height: 40rpx;
}
.expName.ellipsis {
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.expTab .expTime{
  margin-left: 0;
  flex-shrink: 0;
  font-size: 24rpx;
  font-weight: 300;
  color: #626262;
  line-height: 40rpx;
}
.expTab .expInfo{
  font-size: 28rpx;
  font-weight: 400;
  padding: 24rpx 0 0;
  line-height:28rpx;
}
.expTab .expContent{
  line-height: 48rpx;
  font-size: 28rpx;
  font-weight: 300;
  color: #626262;
  margin-top: 16rpx;
  display: block;
}
.expTab .expTip{
  margin-top: 34rpx;
}
.expTab .expTip text{
  margin-right: 12rpx;
  color: #652791;
  font-size: 24rpx;
  font-weight: 300;
  display: inline-block;
  background-color: #EFE9F4;
  padding: 0 20rpx;
  height: 46rpx;
  line-height: 46rpx;
}
.expTab .link{
  line-height: 48rpx;
  word-break: break-all;
  margin-top: 20rpx;
  font-size: 28rpx;
  font-weight: 300;
  color: #626262;
}
.expTab .link text{
  color: #652791;
}
.edit{
  color: #652791;
}
.addpadding{
