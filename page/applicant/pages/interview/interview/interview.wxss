.scroll-box {
  box-sizing: border-box;
  white-space: nowrap;
  overflow-y: hidden;
  overflow-x: auto;
  background: white;
  position: relative;
  width: 100%;
  height: 106rpx;
  padding: 24rpx 0;
}

.scroll-box::-webkit-scrollbar{
  width: 0;
  display: none;
}

.scroll-box:after {
	padding-right:22rpx;
	content: '';
	display: inline-block;
	height: 58rpx;
	vertical-align: middle;
}

.scroll-box:before {
	padding-left:40rpx;
	content: '';
	display: inline-block;
	height: 58rpx;
	vertical-align: middle;
}

.scroll-box .item{
	height:58rpx;
	border-radius:29rpx;
	border:1rpx solid rgba(205,203,207,1);
	line-height: 58rpx;
	text-align: center;
	margin-right: 18rpx;
	font-size:26rpx;
	color:rgba(92,86,93,1);
	padding: 0 43rpx;
	position: relative;
	box-sizing: border-box;
	display: inline-block;
	vertical-align: middle;
	/*transform:translateY(-100%);*/
}

.scroll-box .active{
	border-color: #652791;
	color: #652791;
	pointer-events: none;
}

.scroll-box .red:before{
	position: absolute;
	right: 0;
	top: 0;
	width: 14rpx;
	height: 14rpx;
	border-radius: 50%;
	background: red;
	display: block;
	content: '';
}

