.button-submit {
	height:88rpx;
	background:rgba(245,244,247,1);
	border-radius:48rpx;
	font-size:32rpx;
	font-weight:400;
	color:rgba(205,203,207,1);
	text-align: center;
	line-height: 88rpx;
	height:88rpx;
	border-radius:48rpx;
	font-size:32rpx;
	font-weight:400;
	color:rgba(255,255,255,1);
}

.button-enable {
	height:88rpx;
	background:rgba(101,39,145,1);
	box-shadow:0rpx 20rpx 18rpx 0rpx rgba(101,39,145,0.2);
	border-radius:48rpx;
	font-size:32rpx;
	font-weight:400;
	color:rgba(255,255,255,1);
}

.form {
	display: block;
	width: 100%;
}

.h1 {
	font-size:36rpx;
	font-weight:bold;
	color:rgba(40,40,40,1);
	padding: 56rpx 40rpx 16rpx 40rpx;
	line-height: 1;
	background: white;
}

.p {
	font-size:26rpx;
	font-weight:400;
	color:rgba(146,146,146,1);
	padding: 0 40rpx 0rpx 40rpx;
	background: white;
	padding-bottom: 40rpx;
	margin-bottom: 20rpx;
}

.p text{
	color: #8452A7;
}

.value {
	height:122rpx;
	background:rgba(255,255,255,1);
	line-height: 122rpx;
	font-size:32rpx;
	font-weight:500;
	box-sizing: border-box;
	padding: 0 40rpx;
	color: #282828;
}

.prefix {
	height:122rpx;
	background:rgba(255,255,255,1);
	line-height: 122rpx;
	font-size:32rpx;
	font-weight:500;
	box-sizing: border-box;
	padding-left: 40rpx;
	color: #282828;
	float: left;
	display: inline-block;
}

.suffix {
	height:122rpx;
	background:rgba(255,255,255,1);
	line-height: 122rpx;
	font-size:32rpx;
	font-weight:500;
	box-sizing: border-box;
	padding-right: 40rpx;
	color: #282828;
	float: right;
	display: inline-block;
}

.input-li {
	height:122rpx;
	background:rgba(255,255,255,1);
	line-height: 122rpx;
	font-size:32rpx;
	font-weight:500;
	color:rgba(188,188,188,1);
	box-sizing: border-box;
	position: relative;
	background: white;
	margin-top: 20rpx;
}

.input-li:after {
	content: '';
	display: block;
	left: 40rpx;
	right: 40rpx;
	bottom: 0;
	height: 1px;
	position: absolute;
	background: #E8E9EB;
}

.example {
	height:90rpx;
	background:rgba(255,255,255,1);
	line-height: 90rpx;
	font-size:26rpx;
	font-weight:400;
	color:rgba(98,98,98,1);
	padding: 0 40rpx;
	background: white;
}

.write-box{
  padding-top: 30rpx;
  background: white;
  position: relative;
}

.example1 {
	background:rgba(255,255,255,1);
	line-height: 1;
	font-size:26rpx;
	font-weight:400;
	color:rgba(98,98,98,1);
	background: white;
	padding: 50rpx 40rpx;
}

.example1 text{
	color: #652791;
}

.example1 text.gray{
	color: #CDCBCF;
}

.btn-area {
	width: 100%;
	padding: 0 100rpx;
	background: #FBFBFF;
	box-sizing: border-box;
	position: absolute;
	left: 0;
	bottom: 48rpx;
	text-align: center;
}

.btn-area .p{
	display: inline-block;
	font-size:28rpx;
	font-weight:400;
	color:rgba(188,188,188,1);
	margin-bottom: 50rpx;
}

.tips {
	text-align: center;
	font-size:24rpx;
	font-weight:400;
	margin-top: 48rpx;
}

.tips .span{
	color: rgba(146,146,146,1);
	display: inline;
}

.tips .strong{
	color: #652791;
	display: inline;
	margin-left: 16rpx;
}

.question {
	display: inline;
	float: right;
	position: relative;
}

.question i{
	position: absolute;
	left: -40rpx;
	top: 50%;
	transform: translateY(-50%);
}

.over-layout {
	background:rgba(0,0,0,0.6);
	position: fixed;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	z-index: 10000000000;
}

.over-layout .box{
	background:rgba(255,255,255,1);
	border-radius:18rpx;
	position: absolute;
	left: 90rpx;
	top: 50%;
	right: 90rpx;
	transform: translateY(-50%);
	padding: 60rpx 50rpx 56rpx 50rpx;
}

.close-button {
	position: absolute;
	right: 0;
	top: 0;
	color: #CDCBCF;
	padding: 25rpx;
}
.over-layout .label {
	width:140rpx;
	height:58rpx;
	background:rgba(255,220,41,1);
	border-radius:29rpx 35rpx 35rpx 0rpx;
	font-size:30rpx;
	font-weight:400;
	color:rgba(40,40,40,1);
	text-align: center;
	line-height: 58rpx;
}

.over-layout .value {
	line-height: 1.5;
	white-space: pre-wrap;
	font-size:28rpx;
	font-weight:500;
	color:rgba(40,40,40,1);
	line-height:40rpx;
	margin-top: 16rpx;
	white-space:nowrap;
}

.over-layout .dt {
	/*margin-bottom: 64rpx;*/
	position: relative;
}

.over-layout .dt:last-child {
	margin-bottom: 0rpx;
}

.over-layout text {
	color: #652791;
}

.over-layout .h9 {
	height:36rpx;
	font-size:36rpx;
	font-weight:700;
	color:rgba(53,57,67,1);
	line-height:1;
	text-align: center;
	margin-bottom: 56rpx;
}

.over-layout .p3 {
	font-size:30rpx;
	font-weight:300;
	color:rgba(102,102,102,1);
	line-height:44rpx;
}

.over-layout .p2 {
	font-size:30rpx;
	font-weight:400;
	color:rgba(92,86,93,1);
	line-height:44rpx;
	margin-bottom: 20rpx;
}

.scan-button {
	text-align: center;
	font-weight:400;
	color:rgba(255,255,255,1);
	font-size:34rpx;
	font-weight:400;
	color:rgba(101,39,145,1);
	margin-top: 72rpx;
}

.marginB24 {
	margin-bottom: 24rpx;
}

.position-post .marginB24:after {
	display: none;
}

.input-box {
	background:rgba(239,233,244,0.8);
	border-radius:16rpx;
	border:1rpx solid rgba(239,233,244,1);
	margin: 32rpx 35rpx 40rpx 35rpx;
	position: relative;
	box-sizing: border-box;
	padding: 12rpx 16rpx;
}

.input-box .input{
	background:rgba(255,255,255,1);
	border-radius:12rpx;
	font-size:32rpx;
	font-weight:500;
	color:rgba(101,39,145,1);
	padding-left: 24rpx;
	width: calc(100% - 100rpx);
	line-height: 56rpx;
	height: 56rpx;
}

.input-box .icon-sousuo{
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	right: 30rpx;
	color: #652791;
	font-size: 30rpx;
}

.codeError {
	height:33rpx;
	font-size:24rpx;
	font-weight:400;
	color:rgba(237,92,92,1);
	line-height:33rpx;
	padding: 16rpx 40rpx;
	background: white;
}

.btn-area .p4{
	font-size:28rpx;
	font-weight:400;
	color:rgba(101,39,145,1);
	line-height:1.1;
	text-align: center;
}

.btn-area .p4 .icon-xiangyou{
	display: inline-block;
	vertical-align: text-bottom;
}

.company-email {
	height: 100vh;
	position: relative;
}