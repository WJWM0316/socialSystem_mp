.index-bannar image {
  width: 100%;
  height: 400rpx;
  vertical-align: text-top;
  display: block;
  position: absolute;
  top: 0;
  z-index: -2;
}

.default {
  height: 100rpx;
  padding: 0 30rpx;
  border-radius: 16rpx 16rpx 0 0;
  overflow: hidden;
  padding-top: 20rpx;
}

.default .image {
  height: 10rpx;
  position: absolute;
  bottom: 8rpx;
  z-index: 3;
  background: white;
  left: 0;
  width: 692rpx;
  padding-bottom: 8rpx;
}
.fixed {
  position: fixed;
  left: 0;
  right: 0;
  background: #fff;
  z-index: 22;
  padding-top: 20rpx;
}

.fixed:before {
  position: absolute;
  left: -30rpx;
  height: 120rpx;
  top: -20rpx;
  content: '';
  display: block;
  background: #FBFBFF;
  background: transparent;
  right: -30rpx;
}

.ul-tab-bar {
  position: relative;
  height: 100rpx;
  overflow: hidden;
  border-radius: 16rpx 16rpx 0 0;
  z-index: 1;
}


.ul-tab-bar .li {
  font-size:28rpx;
  color:#929292;
  text-align: center;
  position: relative;
  background:white;
  line-height: 80rpx;
  width: 50%;
  float: left;
  display: inline-block;
}
.lookMe {
  display: inline-block;
}
.ul-tab-bar .line {
  background: white;
  position: absolute;
  left: 0;
  right: 0;
  height: 5rpx;
  bottom: 0rpx;
  z-index: 3;
}
.ul-tab-bar .active {
  background: #EFE9F4;
  pointer-events: none;
  font-size:28rpx;
  font-weight:700;
  color:#652791;
}
.ul-tab-bar .active .line {
  background: #F7EBFF;
}
.ul-tab-bar .total {
  width:36rpx;
  height:36rpx;
  background:rgba(255,127,76,1);
  border-radius: 50%;
  line-height: 34rpx;
  text-align: center;
  font-size:24rpx;
  font-weight:700;
  color:rgba(255,255,255,1);
  position: absolute;
  top: 50%;
  right: 20%;
  transform: translate3d(0, -50%, 0);
}

.ul-tab-bar .active:before {
  opacity: 1;
  visibility: visible;
}

.ul-tab-bar .active:after {
  opacity: 1;
  visibility: visible;
}

.list-box {
  position: relative;
  margin: -28rpx 30rpx auto 30rpx;
}
.list-box.adviser {
  margin-bottom: 20rpx;
}
.list-box.normal {
