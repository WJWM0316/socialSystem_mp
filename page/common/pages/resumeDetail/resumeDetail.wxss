.resumeDetail{
  /*background: #F5F4F7;*/
  width: 100%;
  padding: 25rpx 30rpx;
  box-sizing: border-box;
  position: relative;
  min-height: 85vh;
}
.share{
  width: 76rpx;
  height: 68rpx;
  position: absolute;
  top: 30rpx;
  right: 0;
  border-radius: 36rpx 0 0 36rpx;
  background-color: #FFFFFF;
  box-shadow:0rpx 4rpx 14rpx 0rpx rgba(101,39,145,0.18);
  display: flex;
  align-items: center;
  justify-content: center;
}
.avatarBox{
  min-width: 120rpx;
  width: 120rpx;
  height: 120rpx;
  margin-right: 40rpx;
  position: relative;
}
.adviser .avatarBox {
  margin-right: 18rpx;
}
.avatarBox .gender{
  position: absolute;
  bottom: 4rpx;
  right: 4rpx;
  width: 32rpx;
  height: 32rpx;
}
.avatarBox .gender .sexIcon{
  width: 100%;
  height: 100%;
  display: block;
}
.girl{
  background-color: #FE6795;
}
.man{
  background-color: #2878FF;
}
.gender i .icon-xingbiebiaoqian-nan{
  background-color: #2878FF;
}
.avatar{
  min-width: 120rpx;
  border-radius: 50%;
  width: 120rpx;
  height: 120rpx;
  background-color: #ccc;
  box-shadow:0rpx 2rpx 12rpx 0rpx rgba(0,0,0,0.12);
  flex-shrink: 0;
  overflow: hidden;
}
.avatar .img {
  width: 120rpx;
  height: 120rpx;
}

.previewFile {
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 26rpx 40rpx 23rpx;
  box-shadow:0rpx 4rpx 20rpx 0rpx rgba(101,39,145,0.1);
  border-radius:10rpx;
  margin-bottom: 20rpx;
}
.fileName {
  font-size:26rpx;
  font-weight:400;
  color:rgba(40,40,40,1);
  line-height:37rpx;
}
.fileIcon {
  width: 67rpx;
  height: 51rpx;
  margin-right: 20rpx;
  vertical-align:middle;
}
.previewBtn {
  font-size:26rpx;
  font-weight:400;
  color:rgba(101,39,145,1);
  line-height:37rpx;
}
.btnIcon {
  font-size: 26rpx;
  color: #652791;
  margin-left: 10rpx;
}
.personalInfo .top{
  display: flex;
  flex-wrap: nowrap;
  width: 100%;
}
