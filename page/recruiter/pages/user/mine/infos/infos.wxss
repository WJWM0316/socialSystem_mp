top-banner.center {
	position: relative;
}
.top-banner {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 415rpx;
  overflow: hidden;
}
.top-banner.isIphoneX {
	overflow: visible;
}
.top-banner image {
	width: 100%;
}

.page-mine {
	position: relative;
	margin: 32rpx 30rpx 0;
}

.floating-box {
	overflow: hidden;
	background:white;
	border-radius:16rpx;
	box-sizing: border-box;
	padding: 44rpx 40rpx 40rpx 40rpx;
	background:rgba(255,255,255,1);
	box-shadow:0rpx 10rpx 30rpx 0rpx rgba(22,39,77,0.05),0rpx -5rpx 40rpx 0rpx rgba(22,39,77,0.05);
}

.floating-box .header {
	display: flex;
	overflow: hidden;
}

.floating-box .img-box {
	width:164rpx;
	height:164rpx;
	border-radius:50%;
	background: white;
	position: relative;
}

.floating-box .certification-icon {
  position: absolute;
  bottom: 10rpx;
  right: 4rpx;
  width: 48rpx;
  height: 48rpx;
  background-color: #FFDC29;
  line-height: 48rpx;
  text-align: center;
  border-radius: 50%;
  border: 4rpx solid #FFFFFF;
  font-size: 36rpx;
  color: #652791;
  font-weight: 900;
  font-style: italic;
}

.floating-box .content {
	flex-grow: 1;
}

.floating-box .realname {
	width: 400rpx;
	font-size:56rpx;
	font-weight:600;
	color:rgba(40,40,40,1);
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 1;
	overflow: hidden;
}

.floating-box .degree-name {
	font-size:26rpx;
	font-weight:400;
	color:rgba(40,40,40,1);
	line-height:1;
	margin-top: 16rpx;
	max-width: 400rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.floating-box .edit {
	font-size:26rpx;
	font-weight:500;
	color:rgba(101,39,145,1);
	margin-top: 30rpx;
	background: none;
	padding: 0;
	line-height: normal;
	text-align: left;
}
.floating-box .edit:after {
	display: none;
}
.floating-box .share-homepage {
	line-height: 90rpx;
	background:rgba(40,120,255,1);
	box-shadow:0px 20rpx 18rpx 0rpx rgba(24,93,254,0.1);
	border-radius:45rpx;
	margin: 50rpx auto 24rpx auto;
	font-size:32rpx;
	font-weight:400;
	color:rgba(255,255,255,1);
	text-align: center;
	box-sizing: border-box;
	overflow: hidden;
	background:rgba(101,39,145,1);
	box-shadow:0px 20rpx 18rpx 0rpx rgba(101,39,145,0.1);
	border-radius:45rpx;
}

.floating-box .share-homepage-pic {
	line-height: 90rpx;
	background:white;
	border-radius:45rpx;
	font-size:32rpx;
	font-weight:400;
	color:rgba(101,39,145,1);
	text-align: center;
	box-sizing: border-box;
	overflow: hidden;
	border: 1rpx solid #652791;
	box-shadow:0rpx 10rpx 30rpx 0rpx rgba(22,39,77,0.05),0rpx -5rpx 40rpx 0rpx rgba(22,39,77,0.05);
}

.show-off-block {
	background:rgba(255,255,255,1);
	box-shadow:0rpx 10rpx 30rpx 0rpx rgba(22,39,77,0.05),0rpx -5rpx 40rpx 0rpx rgba(22,39,77,0.05);
	border-radius:16rpx;
	margin: 24rpx auto;
	box-sizing: border-box;
	padding: 32rpx 40rpx;
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.show-off-block .first {
	font-size:32rpx;
	font-weight:bold;
	color:rgba(53,57,67,1);
	line-height:32rpx;
}

.show-off-block .first .strong {
	font-size:38rpx;
	font-weight:bold;
	color:#FF7F4D;
  line-height: 32rpx;
  vertical-align: -2rpx;
}

.show-off-block .second {
	font-size:26rpx;
	font-weight:300;
	color:#626262;
	line-height:26rpx;
	margin-top: 16rpx;
}

.show-off-block .second view {
	display: inline-block;
}

.show-off-block .second .strong {
	color: #626262;
	font-weight: 500;
}
.show-off-block .second .influence {
	margin-left: 30rpx;
}
.show-off-block .content {
	display: inline-block;
}

.entry-list.show-off-block .text2 {
  font-size:26rpx;
  font-weight:400;
  color:rgba(101,39,145,1) !important;
  line-height:70rpx;
}
.show-off-block .btn-box .icon-xiangyou {
  color: #CED7DC;
  position: absolute;
  right: 24rpx;
  top: 50%;
  transform: translateY(-50%);
}

.entry-list {
	margin: 24rpx auto;
	box-sizing: border-box;
	box-shadow:0px 10rpx 30rpx 0rpx rgba(22,39,77,0.05);
	border-radius:16rpx;
	overflow: hidden;
}

.entry-list .li {
	background: white;
	line-height: 120rpx;
	overflow: hidden;
	font-size:28rpx;
	font-weight:400;
	position: relative;
	padding: 0 40rpx 0 40rpx;
}
.entry-list .li.toolbox {
	display: flex;
	padding: 42rpx 36rpx;
}
.entry-list .li.toolbox .tool-item {
	flex: 1;
	position: relative;
}
.entry-list .li.toolbox .tool-item.red::before {
	width: 14rpx;
	height: 14rpx;
	border-radius: 50%;
	content: '';
	position: absolute;
	top: 0;
	right: 40rpx;
	background: #FF4949;
}
.entry-list .li.toolbox .tool-item .icon {
	width:80rpx;
	height:80rpx;
	background:rgba(255,255,255,0.01);
  display: block;
	margin: 0 auto;
}
.entry-list .li.toolbox .tool-item .title {
	font-size:26rpx;
	color:#282828;
	line-height:24rpx;
	text-align: center;
	margin-top: 16rpx;
}
.entry-list .li.toolbox .tool-item .txt {
	font-size:22rpx;
	color:#BCBCBC;
	line-height:22rpx;
	text-align: center;
	margin-top: 10rpx;
}
.entry-list .li.toolbox .tool-item .txt .num {
	color: #FF7F4D;
}
.entry-list .li:after {
	position: absolute;
	left: 40rpx;
	right: 40rpx;
	bottom: 0;
	height: 1rpx;
	background: rgba(232,233,235,1);
	display: block;
	content: ''
}

.entry-list .li:last-child:after {
	height: 0;
}

.entry-list .label-icon , .toggleIdentity .label-icon{
	margin-right: 24rpx;
	color: #CED7DC;
	font-size: 34rpx;
}

.entry-list .text, .toggleIdentity .text {
	display: inline-block;
	color: #353943;
	position: relative;
}

.entry-list .text2, .toggleIdentity .text2 {
	display: inline-block;
	color: #353943;
	float: right;
	margin-right: 24rpx;
	max-width: 300rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.entry-list .text2.rz {
  margin-right: 0;
}
.entry-list .not-certified {
	display: inline-block;
	color: #2878FF;
}

.entry-list .has-certified {
	display: inline-block;
	color: #BCBCBC;
}

.entry-list .under-certified {
	display: inline-block;
	color: #F5A623;
}

.copy-right {
	font-size:26rpx;
	font-weight:400;
	color:#929292;
	line-height:1;
	margin: 50rpx;
	text-align: center;
	margin-bottom: 148rpx;
}

.copy-right view {
	display: inline-block;
	border-right: 1px solid #E8E9EB;
	margin-right: 24rpx;
	padding-right: 24rpx;
}

.copy-right view:last-child {
	border-right: unset;
}

.entry-list .icon-xiangyou, .toggleIdentity .icon-xiangyou{
  color: #CED7DC;
  position: absolute;
  right: 24rpx;
  top: 50%;
  transform: translateY(-50%);
}
.show-off-block .icon-xiangyou{
  color:#652791;
  transform: translateY(-50%);
}
.entry-list .color-gray {
	color: #929292 !important;
}

.entry-list .color-pedding {
	color: #652791 !important;
}

.entry-list .color-fail {
	color: #ED5C5C !important;
}

.boxs {
	display: inline-block;
	font-size: 0;
	float: right;
	margin-right: 24rpx;
	line-height: 120rpx;
	position: relative;
}

.entry-list .text.reddot:before {
	position: absolute;
  top: 50%;
  right: -30rpx;
  display: block;
  content: '';
  background: red;
  width:14rpx;
  height:14rpx;
  background:rgba(255,73,73,1);
  border-radius: 50%;
  transform: translateY(-50%);
}

.boxs .people {
	margin-right: -12rpx;
	border-radius: 50%;
	vertical-align: middle;
	width:56rpx;
	height:56rpx;
	border:2rpx solid rgba(255,255,255,1);
	box-sizing: border-box;
}
.boxs .people:last-child {
	margin-right: 0;
}

.sign{
  position: absolute;
  bottom: 4rpx;
  right: 4rpx;
  width: 48rpx;
  height: 48rpx;
}

.color-default {
	color: #929292 !important;
}

.toggleIdentity {
	margin-top: 24rpx;
	border-radius: 16rpx;
	background: white;
	line-height: 120rpx;
	overflow: hidden;
	font-size:28rpx;
	font-weight:400;
	position: relative;
	padding: 0 40rpx 0 40rpx;
	box-shadow:0rpx 8rpx 40rpx 0rpx rgba(16,40,79,0.06);
	margin-bottom: 40rpx;
}

.perfects {
	padding: 0 10rpx;
	height:40rpx;
	background:rgba(101,39,145,0.1);
	border-radius:4rpx;
	font-size:24rpx;
	color:#652791;
	line-height:40rpx;
	text-align: center;
	margin-top: 26rpx;
	display: inline-block;
}