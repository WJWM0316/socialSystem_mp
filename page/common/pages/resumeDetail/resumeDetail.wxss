.resume-from {
  height:68rpx;
  background:rgba(255,255,255,1);
  border-radius:0rpx 0rpx 28rpx 28rpx;
  font-size:28rpx;
  color:rgba(92,86,93,1);
  line-height:68rpx;
  text-align: center;
  position: relative;
}

.resume-from:before {
  height: 93rpx;
  background: #F5F4F7;
  content: '';
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  width: 100%;
  z-index: -1;
}

.resume-from .text4{
  display: inline-block;
  position: relative;
  z-index: 5;
}

.resume-from .icon-left{
  width: 20rpx;
  height: 12rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: -30rpx;
}

.resume-from .icon-right{
  width: 20rpx;
  height: 12rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: -30rpx;
}

.resume-from .icon1 .img01 {
  width: 20rpx;
  height: 15rpx;
}


.resume-from .position {
  display: inline;
  position: relative;
  color: #652791;
}

.resume-from .position .text10{
  position: relative;
  z-index: 1
}

.resume-from .position:after {
  height:6rpx;
  background:#FFDC29;
  border-radius:2rpx;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 4rpx;
  content: '';
  display: block;
  z-index: 0;
}

