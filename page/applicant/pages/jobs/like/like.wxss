.ul {
	padding: 30rpx 30rpx 24rpx 30rpx;
	overflow: hidden;
	position: relative;
}

.office-status-bar{
  box-sizing: border-box;
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  background-color: #FFFFFF;
  box-shadow:0rpx 11rpx 30rpx 0rpx rgba(153,193,214,0.08);
  position: fixed;
  left: 0;
  z-index: 1;
  text-align: center;
}

.office-status-bar::after{
  content: '.';
  display: block;
  opacity: 0;
}

.office-status-bar .item {
  box-sizing: border-box;
  padding: 0 30rpx;
  text-align: center;
  background-color: #FFFFFF;
  margin-left: 10rpx;
  border-radius: 27rpx;
  position: relative;
  font-size:32rpx;
  font-weight:400;
  color:rgba(98,98,98,1);
  display: inline-block;
  margin: 0 75rpx;
}

.office-status-bar .item::after {
  content: '';
  display: block;
  width:32rpx;
  height:8rpx;
  background:#652791;
  border-radius:4rpx;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  opacity: 0;
  visibility: hidden;
}

.office-status-bar .active {
  font-size:32rpx;
  font-weight:600;
  color:rgba(40,120,255,1);
  font-size:32rpx;
  font-weight:600;
  color:rgba(40,40,40,1);
  pointer-events: none;
}

.office-status-bar .active::after {
  opacity: 1;
  visibility: visible;
}

.office-status-bar::after{
  content: '.';
  display: block;
  opacity: 0;
}
