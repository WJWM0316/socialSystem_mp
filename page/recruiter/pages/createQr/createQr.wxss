.button-submit {
	height:88rpx;
	background:#F5F4F7;
	border-radius:48rpx;
	font-size:32rpx;
	color:#CDCBCF;
	text-align: center;
	line-height: 88rpx;
}

.button-enable {
	background:rgba(101,39,145,1);
	box-shadow:0rpx 20rpx 18rpx 0rpx rgba(101,39,145,0.2);
	color:rgba(255,255,255,1);
}

.footer {
	position: fixed;
	left: 0;
	right: 0;
	bottom: 0;
	background: #FBFBFF;
	background:rgba(255,255,255,1);
	box-shadow:0rpx -1rpx 0rpx 0rpx rgba(206,215,220,1);
	opacity:0.96;
	padding: 30rpx 100rpx;
}

.canvas-box .canvas{
	margin: 100rpx auto;
}