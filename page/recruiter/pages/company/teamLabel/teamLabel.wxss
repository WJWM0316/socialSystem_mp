
.tabsPage {
}
.content {
  padding: 36rpx 44rpx;
}
.head {
  color: #282828;
  font-size: 32rpx;
  margin-bottom: 40rpx;
}
.head1 {
  margin-top: 70rpx;
}
.head2 {
  margin-top: 40rpx;
}
.number {
  color: #BCBCBC;
  font-size: 28rpx;
  font-weight: 300;
  margin-left: 16rpx;
  display: inline-block;
}
.number .num {
  color: #FF7F4C;
}
.tabs {
  width: 100%;
  padding: 0 0 20rpx;
  border-bottom: 1rpx solid #CED7DC;
}
.tabs .tab {
  padding: 0 38rpx;
  height: 58rpx;
  line-height: 58rpx;
  text-align: center;
  box-sizing: border-box;
  display:inline-block;
  margin-right: 20rpx;
  border-radius: 29rpx;
  border: 1rpx solid #E8E9EB;
  font-size: 28rpx;
  font-weight: 300;
  color: #929292;
  margin-bottom: 20rpx;
}
.tabs .tab.checked {
  color: #652791;
  border-color: #652791;
}
.myPicker {
  padding: 0 30rpx;
  height: 58rpx;
  line-height: 58rpx;
  display: inline-block;
  border: 1rpx solid #E8E9EB;
  border-radius: 50rpx;
  margin-left: 48rpx;
  text-align: center;
  font-weight: 300;
  color: #929292;
  font-size: 28rpx;
}
.msg {
  font-size: 28rpx;
  line-height: 28rpx;
  text-align: center;
  font-weight: 300;
  color: #353943;
  margin-top: 55rpx;
}
.addLabel {
  width: 294rpx;
  height: 68rpx;
  line-height: 68rpx;
  border-radius: 34rpx;
  border: 1rpx solid #652791;
  color: #652791;
  font-size: 28rpx;
  font-weight: 500;
  text-align: center;
  margin-top: 26rpx;
  margin: 26rpx auto 90rpx;
}
.nextStep, .save {
  width: 580rpx;
  height: 90rpx;
  background: #652791;
  border-radius: 49rpx;
  line-height: 90rpx;
  text-align: center;
  color: #FFFFFF;
  font-size: 32rpx;
  font-weight: 400;
  display: block;
  margin: 0 auto;
}
.customPop {
  width: 100%;
  height: 100%;
  display: flex;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 2;
  justify-content: center;
  align-items: center;
  background: rgba(0,0,0,0.7);
}
.inner {
  width: 650rpx;
  height: 430rpx;
  background: #fff;
  border-radius: 18rpx;
  padding: 60rpx 50rpx;
  box-sizing: border-box;
  position: relative;
}
.colseBtn {
  top: 17rpx;
  right: 10rpx;
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 25rpx;
  color: #BCBCBC;
  display: block;
}
.title {
  margin: 0 0 56rpx;
  line-height: 36rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: #353943;
  text-align: center;
}
.inputBox {
  position: relative;
}
.inputBox .num {
  font-size: 26rpx;
  line-height: 98rpx;
  color: #BCBCBC;
  font-weight: 400;
  position: absolute;
  top: 0;
  right: 30rpx;
}
.customLabel {
  width: 100%;
  height: 98rpx;
  font-size: 28rpx;
  border-radius: 49rpx;
  font-weight: 300;
  padding: 0 30rpx 0 60rpx;
  border: 1rpx solid #E8E9EB;
  background: #F8F8F8;
  box-sizing: border-box;
  margin-bottom: 32rpx;
}
.addBtn {
  width: 100%;
  height: 88rpx;
  background: #652791;
  border-radius: 49rpx;
  color: #fff;
  line-height: 88rpx;
  font-size: 32rpx;
  font-weight: 500;
}