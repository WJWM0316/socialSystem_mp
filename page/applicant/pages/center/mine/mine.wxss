.bgImg {
  width: 100%;
  height: 415rpx;
  display: block;
  position: fixed;
  left: 0;
  top: 0;
  z-index: -1;
  transform: translate3d(0,0,0);
}

.mine .floating-box {
  overflow: hidden;
  background: white;
  border-radius: 16rpx;
  margin: 0 30rpx;
  box-sizing: border-box;
  padding: 40rpx 40rpx 40rpx 40rpx;
  box-shadow:0rpx 10rpx 30rpx 0rpx rgba(22,39,77,0.05);
}

.mine .floating-box .header {
  display: flex;
  justify-content: space-between;
  overflow: hidden;
}

.mine .floating-box .img-box {
  width: 164rpx;
  height: 164rpx;
  border-radius: 50%;
  overflow: hidden;
  background: #BCBCBC url('https://lieduoduo-uploads-test.oss-cn-shenzhen.aliyuncs.com/front-assets/images/pic_dafult_head.png');
  background-size: 100% 100%;
}

.mine .floating-box .content {
  max-width: 400rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
}

.mine .floating-box .realname {
  font-size: 56rpx;
  font-weight: 700;
  color: rgba(40, 40, 40, 1);
  line-height: 56rpx;
}

.mine .floating-box .degree-name {
  font-size: 28rpx;
  font-weight: 300;
  color: rgba(40, 40, 40, 1);
  line-height: 28rpx;
  margin-top: 22rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.mine .floating-box .experience {
	font-size:26rpx;
	font-weight:300;
	color: rgba(40, 40, 40, 1);
	line-height:1;
  margin-top: 16rpx;
}
.experience .item {
  margin-right: 32rpx;
  display: inline-block;
}
.experience .icon {
  font-size: 30rpx;
  color: #CDCBCF;
  margin-right: 12rpx;
}
.mine .floating-box .directions{
  width: 343rpx;
  color: #929292;
  display: block;
  line-height: 36rpx;
}

.mine .floating-box .edit-box{
  color: #626262;
  margin-top: 42rpx;
  font-size: 26rpx;
  font-weight: 400;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.floating-box .edit-box .right-edit{
  font-weight: 500;
  color: #2878FF;
  display: flex;
  align-items: center;
  justify-content: center;
}
.floating-box .edit-box .progress-rate{
  display: inline-block;
  padding: 8rpx 10rpx;
  font-size: 24rpx;
  background:rgba(101,39,145,0.1);
  color: #652791;
  margin-right: 16rpx;
  border-radius: 4rpx;
}

.edit-box .right-edit i{
  font-size: 32rpx;
  margin-right: 4rpx;
}

.edit-box .right-edit .edit-btn{
  display: flex;
  align-items: center;
  justify-content: center;
  color: #652791;
  padding: 0;
  background: none;
  font-size: 26rpx;
  line-height: normal;
}
.edit-btn:after {
  display: none;
}
/* 发简历按钮 */
.mine .floating-box .share-resume {
  line-height: 90rpx;
  background:rgba(101,39,145,1);
  box-shadow:0rpx 20rpx 18rpx 0rpx rgba(101,39,145,0.2);
  border-radius: 45rpx;
  margin: 48rpx auto 24rpx auto;
  font-size: 32rpx;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  box-sizing: border-box;
  overflow: hidden;
}
.mine .floating-box i{
  font-size: 28rpx;
  width: 28rpx;
  height: 28rpx;
}

/* 保存简历到相册按钮 */
.mine .floating-box .save-resume-pic {
  line-height: 90rpx;
  background: white;
  box-shadow:0rpx 10rpx 30rpx 0rpx rgba(22,39,77,0.05);
  border-radius: 45rpx;
  font-size: 32rpx;
  color:rgba(101,39,145,1);
  text-align: center;
  box-sizing: border-box;
  overflow: hidden;
  border:1px solid rgba(101,39,145,1);
}
/* bottom */
.mine .entry-list {
	margin: 24rpx 30rpx;
	background: white;
	padding: 0 40rpx;
	box-sizing: border-box;
	background:rgba(255,255,255,1);
	box-shadow:0px 10rpx 30rpx 0rpx rgba(22,39,77,0.05);
	border-radius:16rpx;
}

.mine .entry-list .li {
	line-height: 120rpx;
	overflow: hidden;
	border-bottom:1rpx solid rgba(232,233,235,1);
	font-size:28rpx;
	font-weight:400;
	position: relative;
  margin-right: -40rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.mine .entry-list .li:last-child {
	border-bottom: unset;
}

.mine .entry-list .iconfont, .toggleIdentity .iconfont {
	margin-right: 24rpx;
	color: #CED7DC;
  font-size: 34rpx;
}

.mine .entry-list .icon-ganxingqu {
  font-size: 30rpx;
}

.mine .entry-list .icon-wodeguanzhux {
  font-size: 32rpx;
}

.mine .entry-list .text, .toggleIdentity .text {
	display: inline-block;
	color: #353943;
}

.mine .entry-list .text2, .toggleIdentity .text2 {
	display: inline-block;
	color: #929292;
	margin-right: 16rpx;
}

.mine .entry-list .appurtenance{
  color: #652791;
}

.mine .entry-list .gray{
  color: #BCBCBC;
}

.mine .entry-list .not-certified {
	display: inline-block;
	color: #2878FF;
}

.mine .entry-list .has-certified {
	display: inline-block;
	color: #BCBCBC;
}

.mine .entry-list .under-certified {
	display: inline-block;
	color: #F5A623;
}

.mine .copy-right {
	font-size:26rpx;
	font-weight:400;
	color:#929292;
	line-height:1;
	margin: 50rpx;
	text-align: center;
}

.mine .copy-right view {
	display: inline-block;
	border-right: 1px solid #E8E9EB;
	margin-right: 24rpx;
	padding-right: 24rpx;
}

.mine .copy-right view:last-child {
	border-right: unset;
}

.mine .entry-list .arrow-box, .toggleIdentity .arrow-box{
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  justify-content: center;
}

.entry-list .arrow-box i{
  font-size: 30rpx;
  margin-right: 40rpx;
}

.toggleIdentity {
  border-radius: 16rpx;
  background: white;
  line-height: 120rpx;
  overflow: hidden;
  font-size:28rpx;
  font-weight:400;
  position: relative;
  padding: 0 0rpx 0 40rpx;
  box-shadow:0rpx 8rpx 40rpx 0rpx rgba(16,40,79,0.06);
  margin: 24rpx 30rpx 40rpx 30rpx;
}

.toggleIdentity .arrow-box{
  float: right;
}

.toggleIdentity .key{
  float: left;
}

.createBox {
  width:690rpx;
  height:98rpx;
  background:#EFE9F4;
  border-radius:0rpx 0rpx 10rpx 10rpx;
  padding: 17rpx 40rpx;
  box-sizing: border-box;
  font-size:28rpx;
  font-weight:300;
  color:#652791;
  line-height:64rpx;
  margin: 0 auto 0;
  position: relative;
}
.createBox .btn {
  width:190rpx;
  height:64rpx;
  background:#652791;
  border-radius:32rpx;
  float: right;
  font-size:28rpx;
  color:#fff;
  line-height:64rpx;
  text-align: center;
}
.createBox .btn .icon {
  font-size: 24rpx;
  margin-left: 10rpx;
}
.createBtn {
  line-height: 90rpx;
  background:rgba(101,39,145,1);
  box-shadow:0rpx 20rpx 18rpx 0rpx rgba(101,39,145,0.2);
  border-radius: 45rpx;
  margin: 116rpx auto 0;
  font-size: 32rpx;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  box-sizing: border-box;
  overflow: hidden;
}

.no-workAge {
  font-size: 28rpx;
  font-weight: 300;
  line-height: 28rpx;
  margin-top: 22rpx;
  overflow: hidden;
  color: #929292;
}