				</view>
				<view
					class="li {{pageList === 'browseMySelf' ? 'active' : ''}}"
					bindtap="ontabClick"
					data-key="browseMySelf">
					看过我的
					<text class="total" wx:if="{{indexShowCount.viewCount}}">{{indexShowCount.viewCount}}</text>
					<view class="line"></view>
				</view>
				<image src="{{cdnImagePath}}line00002.png" class="image" hidden="{{pageList === 'browseMySelf'}}"></image>
				<image src="{{cdnImagePath}}line00001.png" class="image" hidden="{{pageList === 'collectMySelf'}}"></image>
			</view>
		</view>
