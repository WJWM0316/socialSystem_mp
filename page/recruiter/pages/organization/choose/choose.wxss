
.search {
	width: 100%;
	height:108rpx;
	background:#fff;
	box-shadow:0rpx 0rpx 20rpx 0rpx rgba(50,14,59,0.08);
	padding: 16rpx 30rpx 24rpx 30rpx;
	box-sizing: border-box;
	position: fixed;
	left: 0;
  z-index: 1;
}
.search .searchBar {
	padding: 0 10rpx;
	height:68rpx;
	background:#F8F8F8;
	border-radius:34rpx;
	font-size:28rpx;
  text-align: center;
}

.organizationList {
	margin-top: 20rpx;
	padding-top: 108rpx;
	padding-bottom: 180rpx;
}
.list {
	padding: 0 0 0 40rpx;
	background: #FFFFFF;
}

.list-item {
	height:120rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	border-bottom: 1rpx solid #E8E9EB;
}
.list-item:last-child {
	border: none;
}
.name {
	width: 450rpx;
	font-size:28rpx;
	font-weight:400;
	color:#282828;
	line-height:36rpx;
}
.radio {
  width:45rpx;
  height:45rpx;
  line-height: 45rpx;
  text-align: center;
  border-radius: 50%;
  border: 1rpx solid #BCBCBC;
  display: inline-block;
  transform: scale(0.8);
  font-size: 20rpx;
  font-weight: 500;
  margin-right: 30rpx;
}
.radio-checked {
  border: 1rpx solid #652791;
  background: #652791;
  color: white;
}

.h11 {
	font-size:36rpx;
	font-weight:bold;
	color:rgba(40,40,40,1);
	line-height: 1;
	max-width: 450rpx;
	height:36rpx;
	font-size:36rpx;
	font-weight:700;
	line-height:36rpx;
}

.h111 {
	height:36rpx;
	font-size:36rpx;
	font-weight:700;
	color:rgba(40,40,40,1);
	line-height:36rpx;
	margin: 80rpx 40rpx 35rpx 40rpx;
}

.h1111 {
	font-size:36rpx;
	font-weight:bold;
	color:rgba(40,40,40,1);
	line-height: 1;
	height:36rpx;
	font-size:36rpx;
	font-weight:700;
	line-height:36rpx;
	width: 100%;
}

.h22 {
	width: 450rpx;
	font-size: 28rpx;
	line-height: 36rpx;
	color:rgba(98,98,98,1);
	margin: 15rpx 0;
}

.h222 {
	max-width: 450rpx;
	font-size:26rpx;
	font-weight:300;
	color:rgba(146,146,146,1);
	line-height:26rpx;
	margin-left: 40rpx;
}

.h22 .strong{
	display: inline;
	color: rgba(101,39,145,0.8);
}

.h11 .strong{
	display: inline;
	color: rgba(101,39,145,0.8);
	font-size:26rpx;
}

.h1111 .strong{
	display: inline;
	color: rgba(101,39,145,0.8);
	font-size:26rpx;
	float: right;
}

.list-item1 {
	position: relative;
	margin: 43rpx 40rpx 32rpx 40rpx;
}

.list-item2 {
	position: relative;
	display: flex;
	height: 115rpx;
	align-items: center;
	justify-content: space-between;
	margin-bottom: 50rpx;
	margin: 0 40rpx;
}

.list-item3 {
	position: relative;
	display: flex;
	height: 120rpx;
	align-items: center;
	justify-content: space-between;
	background: white;
}

.input110{
	width: 100%;
	padding: 0 40rpx;
	height: 70rpx;
	font-size: 28rpx;
	box-sizing: border-box;
	height:98rpx;
	border-radius:49rpx;
	border:1rpx solid rgba(232,233,235,1);
	font-weight:300;
	color:#282828;
	margin: 36rpx 0rpx 32rpx 0rpx;
	background: #E8E9EB;
}
.orgul{
	padding-top: 0;
}

.bottomBox {
	position: fixed;
	left: 0;
	bottom: 0;
	padding: 30rpx 100rpx;
	background: #fff;
	width: 100%;
	box-sizing: border-box;
	box-shadow:0px -1rpx 0rpx 0rpx rgba(206,215,220,1);
	opacity:0.96;
}
.button-submit {
	height: 88rpx;
	background:#F5F4F7;
	border-radius:48rpx;
	font-size:32rpx;
	font-weight:400;
	color:#CDCBCF;
	text-align: center;
	line-height: 88rpx;
}

.button-enable {
	background:rgba(101,39,145,1);
	box-shadow:0rpx 20rpx 18rpx 0rpx rgba(101,39,145,0.2);
	color:rgba(255,255,255,1);
}
