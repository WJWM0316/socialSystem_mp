.tab-bar {
  position: relative;
  height: 98rpx;
  line-height: 98rpx;
  overflow: scroll;
  z-index: 1;
  font-size:28rpx;
  font-weight:300;
  color:rgba(40,40,40,1);
  background: white;
  padding-left: 60rpx;
  box-sizing: border-box;
  background: white;
  white-space: nowrap;
  position: relative;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch;
  width: calc(100vw - 160rpx - 48rpx);
  box-sizing: border-box;
  float: left;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
  display: none;
}

.tab-bar .li {
  position: relative;
  margin-right: 40rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  line-height: 98rpx;
  overflow: hidden;
  max-width: 180rpx;
  text-align: center;
  display: inline-block;
  vertical-align: middle;
}

.tab-bar .li:before {
  width:30rpx;
  height:6rpx;
  background:rgba(255,220,41,1);
  border-radius:3rpx;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 2rpx;
  content: '';
  display: block;
  opacity: 0;
  transition: all ease .4s;
}

.tab-bar .active {
  color: #282828;
  font-weight: 700;
  pointer-events: none;
}

.tab-bar .active:before {
  opacity: 1;
}

.lookMe {
  display: inline-block;
}
.tab-bar .total {
  width:36rpx;
  height:36rpx;
  background:rgba(255,127,76,1);
  border-radius: 50%;
  line-height: 34rpx;
  text-align: center;
  font-size:24rpx;
  font-weight:700;
  color:rgba(255,255,255,1);
  position: absolute;
  top: 50%;
  right: 20%;
  transform: translate3d(0, -50%, 0);
}

.tab-bar .active:before {
  opacity: 1;
  visibility: visible;
}

.tab-bar .active:after {
  opacity: 1;
  visibility: visible;
}

