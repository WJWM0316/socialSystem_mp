.fixed-bar {
  display: flex;
  line-height: 98rpx;
	background:rgba(255,255,255,1);
	box-shadow:0px 11rpx 30rpx 0rpx rgba(153,193,214,0.08);
	position: relative;
	z-index: 2;
}

.fixed-bar .li {
  flex-grow: 1;
	font-size:32rpx;
	font-weight:400;
	/*color: #652791;*/
	text-align: center;
	position: relative;
}

.fixed-bar .li .span{
	display: inline-block;
	position: relative;
	line-height: 1;
	vertical-align: middle;
}

.fixed-bar .red-dot .span:before {
  position: absolute;
  top: -15rpx;
  right: -15rpx;
  display: block;
  content: '';
  background: red;
  width:14rpx;
	height:14rpx;
	background:rgba(255,73,73,1);
	border-radius: 50%;
}

.fixed-bar .li:before {
  position: absolute;
  display: block;
  content: '';
	bottom: 0;
	left: 50%;
	transform: translateX(-50%);
	width:32rpx;
	height:8rpx;
	background: #652791;
	border-radius:4rpx;
	opacity: 0;
	visibility: hidden;
	transition: all ease .4s;
}

.fixed-bar .li.active {
	pointer-events: none;
	font-weight: bold;
}

.fixed-bar .li.active:before {
	opacity: 1;
	visibility: visible;
}

