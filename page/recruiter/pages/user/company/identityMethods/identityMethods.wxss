.identityMethods .header {
	padding: 56rpx 40rpx 40rpx 40rpx;
	background: white;
}

.identityMethods .h1 {
	height:36rpx;
	font-size:36rpx;
	font-weight:500;
	color:rgba(40,40,40,1);
	line-height:36rpx;
	margin-bottom: 16rpx;
}

.identityMethods .h2 {
	height:26rpx;
	font-size:26rpx;
	font-weight:400;
	color:rgba(92,86,93,1);
	line-height:26rpx;
}

.identityMethods .ul {
	/*background: white;*/
}

.identityMethods .li {
	display: flex;
	justify-content: center;
  align-items: center;
  overflow: hidden;
  padding: 0 40rpx;
  margin-top: 20rpx;
  background: white;
  position: relative;
  height: 200rpx;
}

.identityMethods .icon-box {
	width: 78rpx;
	height: 78rpx;
	border-radius: 50%;
	background: #F5F4F7;
	line-height: 78rpx;
	text-align: center;
	color: #652791;
}

.identityMethods .content {
	flex-grow: 1;
	margin-left: 28rpx;
}

.identityMethods .h3 {
	height:32rpx;
	font-size:32rpx;
	font-weight:500;
	color:rgba(40,40,40,1);
	line-height:32rpx;
	margin-bottom: 16rpx;
}

.identityMethods .h4 {
	height:26rpx;
	font-size:26rpx;
	font-weight:300;
	color:rgba(98,98,98,1);
	line-height:26rpx;
}

.identityMethods .h5 {
	height:26rpx;
	font-size:26rpx;
	font-weight:300;
	color:rgba(98,98,98,1);
	line-height:26rpx;
	margin-top: 16rpx;
}

.identityMethods .arrow-box {
	position: absolute;
	right: 40rpx;
	top: 50%;
	transform: translateY(-50%);
	background: white;
}

.identityMethods .arrow-box .icon-xiangyou{
	color: #CDCBCF;
	display: inline-block;
	vertical-align: text-bottom;
}

.identityMethods .arrow-box text{
	font-size:26rpx;
	font-weight:500;
	color:rgba(101,39,145,1);
	margin-right: 10rpx;
}

.quickly-entry {
	background:rgba(255,255,255,1);
	box-shadow:0rpx 0rpx 30rpx 0rpx rgba(22,39,77,0.05);
	border-radius:10rpx;
	overflow: hidden;
	margin: 20rpx 30rpx;
  box-sizing: border-box;
  padding: 55rpx 30rpx;
  position: relative;
}

.quickly-entry .bgImg {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	z-index: 0;
}
.quickly-entry .button {
	width:160rpx;
	height:64rpx;
	line-height: 64rpx;
	text-align: center;
	background:rgba(255,220,41,1);
	box-shadow:0rpx 6rpx 20rpx -5rpx rgba(255,220,41,0.8);
	border-radius:32rpx;
	font-size:26rpx;
	font-weight:400;
	color:rgba(40,40,40,1);
	position: relative;
	z-index: 2;
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	right: 30rpx;
}

.quickly-entry .h6 {
	height:32rpx;
	font-size:36rpx;
	font-weight:500;
	color:rgba(40,40,40,1);
	line-height:32rpx;
	margin-bottom: 14rpx;
}

.quickly-entry .h7 {
	height:28rpx;
	font-size:28rpx;
	font-weight:400;
	color:rgba(98,98,98,1);
	line-height:28rpx;
}

.tips {
	position: fixed;
	left: 0;
	right: 0;
	bottom: 48rpx;
	text-align: center;
	font-size:24rpx;
	font-weight:400;
}

.tips .span{
	color: rgba(146,146,146,1);
	display: inline;
}

.tips .strong{
	color: #652791;
	display: inline;
	margin-left: 16rpx;
}