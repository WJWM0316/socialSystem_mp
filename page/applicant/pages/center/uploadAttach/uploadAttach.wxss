.un-upload {
  padding: 0 40rpx;
}

.un-upload .upload-item {
  height:114rpx;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 6rpx 20rpx 0rpx rgba(50,14,59,0.07);
  border-radius:16rpx;
  line-height: 114rpx;
  margin-bottom: 28rpx;
  box-sizing: border-box;
  padding: 0 32rpx;
}

.un-upload .icon-box {
  width:56rpx;
  height:56rpx;
  display: inline-block;
  vertical-align: middle;
  line-height: 56rpx;
  text-align: center;
  position: relative;
  top: -2rpx;
}

.un-upload .icon-box .icon-weixin-mian{
  color: #4BB134;
  font-size: 56rpx;
}

.un-upload .icon-box .icon-shouji-mian{
  color: #17B8D2;
  font-size: 56rpx;
}

.un-upload .text {
  display: inline-block;
  margin-left: 16rpx;
  color: #320E3B;
}

.un-upload .icon-recommand {
  width:72rpx;
  height:32rpx;
  background:rgba(255,127,76,1);
  border-radius:18rpx 18rpx 18rpx 0rpx;
  line-height: 32rpx;
  font-size:24rpx;
  color:rgba(255,255,255,1);
  text-align: center;
  display: inline-block;
  position: relative;
  top: -1rpx;
  margin-left: 8rpx;
}

.un-upload  .tips11 {
  height:24rpx;
  font-size:24rpx;
  color:rgba(146,146,146,1);
  line-height:24rpx;
  text-align: center;
  bottom: 114rpx;
  left: 0;
  right: 0;
  position: fixed;
}

.un-upload .h11{
  height:60rpx;
  font-size:42rpx;
  font-weight:700;
  color:rgba(34,41,44,1);
  line-height:60rpx;
  margin-top: 50rpx;
}

.un-upload .p11{
  font-size:26rpx;
  color:rgba(146,146,146,1);
  line-height:38rpx;
  margin-top: 4rpx;
  margin-bottom: 60rpx;
}

.uploading {
  text-align: center;
  margin-top: 210rpx;
}

.uploading .img-box{
  width:146rpx;
  height:146rpx;
  display: inline-block;
  position: relative;
  overflow: hidden;
  vertical-align: top;
}


.uploading .img-box image{
  width: 100%;
  height: 100%;
}

.uploading .file-name {
  height:40rpx;
  font-size:30rpx;
  color:rgba(40,40,40,1);
  line-height:40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 30rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
}

.uploading .file-size {
  height:37rpx;
  font-size:26rpx;
  font-weight:300;
  color:rgba(92,86,93,1);
  line-height:37rpx;
  margin-top: 16rpx;
}

.uploading .progress {
  position: relative;
  margin-top: 80rpx;
  overflow: hidden
}

.uploading .progress-text {
  height:40rpx;
  font-size:28rpx;
  color:rgba(92,86,93,1);
  line-height:40rpx;
  margin-bottom: 20rpx;
}

.uploading .progress-bar {
  overflow: hidden;
  display: inline-block;
  position: relative;
  width:604rpx;
  height:16rpx;
  background:rgba(248,248,248,1);
  border-radius:8rpx;
  vertical-align: top;
}

.uploading .progress-bar .active{
  background-color:red;
  position: absolute;
  left: 0;
  height: 100%;
  background:rgba(255,220,41,1);
  border-radius:8rpx;
}

.uploading .upload-tips1 {
  margin-top: 30rpx;
  font-size:28rpx;
  color:rgba(146,146,146,1);
  line-height:40rpx;
}

.uploading .upload-tips2 {
  margin-top: 80rpx;
  width:466rpx;
  height:72rpx;
  background:#fff4e5;
  border-radius:12rpx;
  line-height: 72rpx;
  display: inline-block;
}

.uploading .icon-err {
  margin-right: 22rpx;
  display: inline-block;
  vertical-align: top;
  color: rgba(255,127,77,1);
}

.uploading .err-class {
  display: inline-block;
  vertical-align: middle;
  font-size:28rpx;
  color:rgba(255,127,77,1);
  line-height: 1.1;
  height: 40rpx;
}

.re-upload {
  height:98rpx;
  background:rgba(101,39,145,1);
  border-radius:48rpx;
  position: fixed;
  left: 85rpx;
  right: 85rpx;
  font-size:32rpx;
  color:rgba(255,255,255,1);
  line-height: 98rpx;
  text-align: center;
  position: fixed;
  bottom: 170rpx;
}

.has-upload .ul {
  overflow: hidden;
  margin: 0 80rpx;
}

.has-upload .label {
  width:140rpx;
  height:58rpx;
  background:rgba(255,220,41,1);
  border-radius:29rpx 35rpx 35rpx 0rpx;
  font-size:30rpx;
  font-weight:400;
  color:rgba(40,40,40,1);
  text-align: center;
  line-height: 58rpx;
}

.has-upload .value {
  line-height: 1.5;
  white-space: pre-wrap;
  font-size:28rpx;
  font-weight:500;
  color:rgba(40,40,40,1);
  line-height:40rpx;
  margin-top: 16rpx;
}

.has-upload .li {
  margin-bottom: 64rpx;
  position: relative;
}

.has-upload .li:last-child {
  margin-bottom: 0rpx;
}

.has-upload .footer {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  background: white;
}

.has-upload .p {
  font-size:26rpx;
  font-weight:400;
  color:rgba(98,98,98,1);
  margin-bottom: 32rpx;
  text-align: center;
  height: 37rpx;
  line-height: 37rpx;
}

.has-upload .more-action {
  position: relative;
  line-height: 182rpx;
  text-align: right;
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  height: 100%;
  box-sizing: border-box;
}

.has-upload .more-action .circle{
  width:8rpx;
  height:8rpx;
  background:rgba(204,215,221,1);
  border-radius: 50%;
  /* left: 10rpx; */
  display: inline-block;
  vertical-align: middle;
  margin-right: 4rpx;
}

