.button {
  height:90rpx;
  background:rgba(101,39,145,1);
  box-shadow:0rpx 16rpx 30rpx 0rpx rgba(101,39,145,0.3);
  border-radius:48rpx;
  line-height: 90rpx;
  text-align: center;
  font-size:32rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  margin: 0 80rpx 90rpx;
}

.has-upload .img {
  width:262rpx;
  height:198rpx;
  display: inline-block;
  margin-top: 90rpx;
}

.has-upload .infos {
  text-align: center;
}

.has-upload .none {
  text-align: center;
  margin-bottom: 120rpx;
}

.has-upload .has {
  text-align: center;
  margin-bottom: 60rpx;
}

.has-upload .tips {
  text-align: center;
  font-size:32rpx;
  font-weight:500;
  color:rgba(101,39,145,1);
  line-height: 1;
  margin-top: 33rpx;
}

.has-upload .item {
  background:rgba(255,255,255,1);
  border-bottom: 1px solid #E8E9EB;
  border-top: 1px solid #E8E9EB;
  padding: 40rpx 50rpx 40rpx 0;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}

.has-upload .img-box {
  width:100rpx;
  height:100rpx;
  border-radius:10rpx;
  overflow: hidden;
  margin-right: 20rpx;
}

.has-upload .img-box image{
  width: 100%;
  height: 100%;
  margin: 0;
}

.has-upload .content {
  flex: 1;
}

.has-upload .arrow-box {
  flex: 1;
}

.has-upload .attach-box {
  margin: 0 40rpx;
  /* padding-bottom: 273rpx; */
  overflow: hidden;
  position: relative;
}

.has-upload .filename {
  font-size:28rpx;
  font-weight:500;
  color:rgba(40,40,40,1);
  line-height: 36rpx;
  margin-bottom: 10rpx;
  max-width:450rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block;
}

.has-upload .date {
  font-size:26rpx;
  font-weight:300;
  color:rgba(98,98,98,1);
  line-height: 1;
}

.has-upload .icon-xiangyou {
  color: #CED7DC;
}

.has-upload .bg {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: white;
  z-index: -1;
}

.action-menu .mask{
  width:100vw;
  height:100vh;
  background:rgba(0,0,0,.5);
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 10000;
}

.action-menu .content{
  width:100vw;
  border-radius:24rpx 24rpx 0rpx 0rpx;
  position: fixed;
  bottom: 0;
  right: 0;
  left: 0;
  z-index: 10001;
  transform: translateY(100%);
  transition: all ease .2s;
  opacity: .5;
  overflow: hidden;
  background: #FBFBFF;
}

.action-menu .show{
  transform: translateY(0%);
  opacity: 1
}

.action-menu .ul{
  position: relative;
  z-index: 2;
  position: relative;
  z-index: 2;
  overflow: hidden;
  overflow-y: scroll;
  width: 100vw;
  box-sizing: border-box;
}

.action-menu .ul .li{
  height:100rpx;
  line-height: 100rpx;
  /* border-bottom: 1rpx solid #E8E9EB; */
  text-align: center;
  background: white;
  font-size:32rpx;
  color:rgba(40,40,40,1);
  position: relative;
}

.action-menu .ul .btn-preview::after,
.action-menu .ul .btn-reupload::after{
  height:1rpx;
  background: #dcdcdc;
  content: '';
  display: block;
  left: 0;
  right: 0;
  bottom: 0;
  position: absolute;
  transform: scaleY(.5)
}

.action-menu .ul .btn-delete {
  margin-bottom: 20rpx;
  border-bottom: 0;
  color: #FF2E3E;
}

.action-menu .ul .btn-cancle {
  margin-bottom: 36rpx;
  border-bottom: 0;
}

.hidden {
  overflow-y: hidden;
}