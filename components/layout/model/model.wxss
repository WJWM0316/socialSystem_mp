.model .mask {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  z-index: 20;
  background: rgba(0, 0, 0, 0.5);
}
 
.model .box {
  position: fixed;
  top: 50%;
  left: 50rpx;
  right: 50rpx;
  z-index: 21;
  background: #fff;
  transform: translateY(-50%);
  border-radius: 16rpx;
  box-sizing: border-box;
  padding: 40rpx 50rpx;
}
 
.model .title {
  padding: 30rpx 40rpx 30rpx 40rpx;
  text-align: center;
  height:44rpx;
  font-size:36rpx;
  font-weight:700;
  color:rgba(40,40,40,1);
  line-height:44rpx;
  padding-top: 10rpx;
  padding-bottom: 20rpx;
}
 
.model .body {
  min-height: 90rpx;
  display: flex;
  text-align: center;
}

.model .footer {
  display: flex;
  text-align: center;
  font-size:34rpx;
  color:rgba(188,188,188,1);
}

.button-cancle {
  color: #BCBCBC;
  flex: 1;
  border-right: 1rpx solid #e5e5e6;
  height: 42rpx;
  line-height: 42rpx;
}

.button-confirm {
  color: #652791;
  flex: 1;
  height: 42rpx;
  line-height: 42rpx;
}

.button-active {
  height:98rpx;
  background:rgba(110,34,150,1);
  border-radius:49rpx;
  font-size:32rpx;
  color:rgba(255,255,255,1);
  line-height: 98rpx;
}