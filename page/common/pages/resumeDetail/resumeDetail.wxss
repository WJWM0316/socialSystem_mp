.personalInfo .info{
  font-size: 26rpx;
  font-weight: 300;
  color: #282828;
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.personalInfo .info .name{
  font-size: 46rpx;
  font-weight: 500;
  line-height: 46rpx;
  height: 46rpx;
  margin-bottom: 10rpx;
  display: flex;
  justify-content: space-between;
  align-content: center;
}

.personalInfo .info .name .filter {
  width: 108rpx;
  display: inline-block;
  overflow-x: hidden;
}
.invisible {
  width: 172rpx;
  height: 72rpx;
  display: block;
}
.adviser .companyName {
  padding-left: 22rpx;
  margin-top: 6rpx;
}
.experience{
  font-size: 24rpx;
  font-weight: 300;
  color: #282828;
  margin: 37rpx 0 0 0;
  
}
.experience view{
  white-space: nowrap;
  margin-right: 32rpx;
  display: inline-block;
  margin-bottom: 20rpx;
}
.experience i {
  color: #CDCBCF;
  font-size: 28rpx;
  margin-right: 12rpx;
}
.experience .status{
  height: 38rpx;
  line-height: 38rpx;
  color: #652791;
  background-color: #EFE9F4;
  padding: 0 18rpx !important;
