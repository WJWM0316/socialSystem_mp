.ul {
  padding: 118rpx 30rpx 0 30rpx;
  position: relative;
  z-index: 0;
}

.ul .li {
  background:rgba(255,255,255,1);
  border-radius:10rpx;
  position: relative;
  padding: 32rpx 30rpx 34rpx 30rpx;
}

.ul .li .row-one {
  display: flex;
  align-items: center;
  position: relative;
  border-bottom: 1rpx solid #E8E9EB;
  justify-content: center;
  align-items: center;
  height: 120rpx;
  margin-bottom: 30rpx;
  padding-bottom: 24rpx;
}

.ul .li .img-box {
  width:120rpx;
  height:120rpx;
  box-shadow:0rpx -5rpx 40rpx 0rpx rgba(22,39,77,0.05);
  border-radius:50%;
  position: absolute;
  top: 0;
  left: 0;
  border: 4rpx solid white;
  box-sizing: border-box;
  overflow: hidden;
}

.ul .li .img-box image{
  width: 100%;
  height: 100%;
}

.ul .li .content {
  margin-left: 144rpx;
  flex: 1;
}

.ul .li .header {
  max-width:500rpx;
  height:36rpx;
  line-height:36rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  position: relative;
}

.ul .li .header .name {
  font-size:36rpx;
  font-weight:600;
  color:rgba(40,40,40,1);
  display: inline;
}

.ul .li .degree:before {
  background: #282828;
  position: absolute;
  width: 8rpx;
  height: 8rpx;
  border-radius: 50%;
  display: block;
  content: '';
  left: 10rpx;
  top: 50%;
  transform: translateY(-50%);
}

.ul .li .header .degree{
  font-size:28rpx;
  color:#282828;
  position: relative;
  display: inline;
  padding-left: 30rpx;
  top: -2px;
}

.ul .li .infos1 {
  display: inline-block;
  max-width:340rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.ul .li .infos2 {
  max-width:490rpx;
  height:28rpx;
  font-size:26rpx;
  font-weight:300;
  color:rgba(98,98,98,1);
  line-height:28rpx;
  margin-top: 14rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.ul .li .time {
  font-size:24rpx;
  font-weight:400;
  color:#BCBCBC;
  display: inline-block;
  float: right;
}
.ul .li .time.color {
  color: #652791;
}
.ul .li .icon {
  color: #CDCBCF;
  font-size: 32rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 0;
}

.ul .li .icon-mianshidizhi {
  font-size: 30rpx;
}
.ul .li .row-two {
  max-width:calc(100% - 56rpx);
  line-height: 28rpx;
  font-size:26rpx;
  color:#5C565D;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  position: relative;
  padding-left: 45rpx;
  margin-bottom: 26rpx;
}

.ul .li .row-three {
  max-width:calc(100% - 56rpx);
  line-height: 28rpx;
  font-size:26rpx;
  color:#5C565D;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-left: 45rpx;
  position: relative;
}

.history .formIdBtn {
  padding: 0;
  background: none;
  text-align: left;
  line-height: normal;
  overflow: unset;
  margin-bottom: 20rpx;
  box-shadow:0rpx -5rpx 40rpx 0rpx rgba(22,39,77,0.05);
}

.fixed-box .down-show {
  height:98rpx;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 18rpx 20rpx 0rpx rgba(101,39,145,0.04);
  padding: 0 40rpx;
  font-size:28rpx;
  font-weight:400;
  color:rgba(92,86,93,1);
  display: flex;
  align-items: center;
  position: fixed;
  z-index: 3;
  left: 0;
  right: 0;
}

.fixed-box .down-show-active {
  color: #652791;
  font-weight: 700;
}

.fixed-box .down-show .key {
  position: relative;
  display: inline-block;
  line-height: 1;
  margin-right: 74rpx;
}

.fixed-box .down-show .iconfont{
  transform: rotate(0deg);
  position: absolute;
  right: -35rpx;
  top: 50%;
  width: 32rpx;
  height: 32rpx;
  margin-top: -16rpx;
  transition: all;
  color: #929292;
}

.fixed-box .down-show .rotate180{
  transform: rotate(180deg);
}

.fixed-box .child-box {
  background:rgba(255,255,255,1);
  box-shadow:0rpx 11rpx 30rpx 0rpx rgba(0,0,0,0.04);
  border-radius:0rpx 0rpx 10rpx 10rpx;
  padding-top: 98rpx;
  z-index: 2;
  position: fixed;
  left: 0;
  right: 0;
}

.fixed-box .child-box .p1{
  height:24rpx;
  font-size:24rpx;
  font-weight:300;
  color:rgba(146,146,146,1);
  line-height:24rpx;
  display: inline-block;
}

.fixed-box .child-box .h2{
  height:28rpx;
  font-size:28rpx;
  font-weight:700;
  color:rgba(40,40,40,1);
  line-height:28rpx;
  margin-top: 64rpx;
}

.fixed-box .child-box .box4{
  padding-top: 26rpx;
}

.fixed-box .child-box .box4 .item{
  display: inline-block;
  width:138rpx;
  height:58rpx;
  border-radius:29rpx;
  border:1rpx solid rgba(205,203,207,1);
  margin-right: 18rpx;
  text-align: center;
  line-height: 58rpx;
  font-size:26rpx;
  color:#626262;
}

.fixed-box .child-box .box4 .active{
  color:rgba(101,39,145,1);
  border-color: rgba(101,39,145,1);
  border-width: 1px;
}

.fixed-box .child-box .h3{
  height:28rpx;
  font-size:28rpx;
  font-weight:700;
  color:rgba(40,40,40,1);
  line-height:28rpx;
  padding-top: 64rpx;
}

.fixed-box .child-box .time{
  padding: 0 40rpx;
}

.fixed-box .child-box .select-range{
  height:40rpx;
  font-size:28rpx;
  font-weight:500;
  line-height:40rpx;
  padding: 32rpx 0 88rpx 0;
  text-align: left;
  position: relative;
}

.fixed-box .child-box .select-range .start{
  display: inline;
  margin-right: 20rpx;
  color:#929292;
  position: relative;
  margin-left: -8rpx;
}

.fixed-box .child-box .select-range .picker{
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  opacity: 0;
}

.fixed-box .child-box .select-range .end{
  display: inline;
  margin-right: 20rpx;
  color:#929292;
  position: relative;
}

.fixed-box .child-box .select-range .separator{
  display: inline;
  margin-right: 20rpx;
  color: #5C565D;
}

.fixed-box .child-box .select-range .icon{
  width: 32rpx;
  height: 32rpx;
}

.fixed-box .child-box .select-range .active {
  color:rgba(101,39,145,1);
}

.fixed-box .mask{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,.3);
  z-index: 1;
}

.fixed-box {}

.fixed-box .ul10 {
  padding: 64rpx 0 53rpx 0;
}

.fixed-box .ul10 .item10{
  height:58rpx;
  line-height: 58rpx;
  text-align: center;
  border-radius:29rpx;
  border:1rpx solid rgba(205,203,207,1);
  font-size:26rpx;
  font-weight:300;
  color:rgba(98,98,98,1);
  display: inline-block;
  margin-right: 18rpx;
  margin-bottom: 22rpx;
  padding: 0 48rpx;
  min-width: 148rpx;
  box-sizing: border-box;
}

.fixed-box .button-box {
  display: flex;
  padding-bottom: 40rpx;
}

.fixed-box .button-reset {
  width:260rpx;
  height:88rpx;
  line-height: 88rpx;
  text-align: center;
  background:rgba(205,203,207,1);
  border-radius:50rpx;
  font-size:32rpx;
  color:rgba(255,255,255,1);
  margin-right: 30rpx;
}

.fixed-box .button-sure {
  width:260rpx;
  height:88rpx;
  line-height: 88rpx;
  text-align: center;
  border-radius:50rpx;
  font-size:32rpx;
  color:rgba(255,255,255,1);
  background:rgba(101,39,145,1);
  border-radius:49px;
  flex: 1;
}
.jump {
  width: 430rpx;
  height: 98rpx;
  background:rgba(255,220,41,1);
  box-shadow:0rpx 4rpx 20rpx 0rpx rgba(232,201,40,0.47);
  border-radius:49rpx;
  margin: 44rpx auto 0;
  font-size:30rpx;
  font-weight:400;
  color:rgba(40,40,40,1);
  line-height: 98rpx;
}

