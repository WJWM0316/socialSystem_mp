.interest {
	min-height: 100vh;
	background: white;
	width: 100vw;
	z-index: 1;
	position: relative;
}

.box {
	/*border-top: 24rpx solid #FBFBFF;*/
	overflow: hidden;
	padding-top: 32rpx;
}

.card {
	margin: 0 40rpx;
	height: 300rpx;
	position: relative;
}

.card .filter {
	width:588rpx;
	height:26rpx;
	background:rgba(213,213,213,1);
	border-radius:13rpx;
	filter:blur(15rpx);
	position: absolute;
	left: 50%;
	transform: translateX(-50%);
	z-index: -1;
	bottom: -13rpx;
}

.card .bg{
	width: 670rpx;
	height: 300rpx;
	display: block;
}

.card .overlay {
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	box-sizing: border-box;
	padding: 32rpx;
	z-index: 3;
}

.card .overlay .button {
	width:200rpx;
	height:64rpx;
	background:linear-gradient(90deg,rgba(255,234,125,1) 0%,rgba(255,220,41,1) 100%);
	border-radius:32rpx;
	font-size:28rpx;
	font-weight:500;
	color:rgba(101,39,145,1);
	text-align: center;
	line-height: 64rpx;
	display: inline-block;
}

.card .overlay .header {
	display: flex;
	justify-content: center;
  align-items: center;
}

.card .overlay .footer {
	position: absolute;
	left: 32rpx;
	right: 32rpx;
	bottom: 32rpx;
	text-align: right;
}

.card .overlay .img-box {
	width:120rpx;
	height:120rpx;
	border:4rpx solid rgba(255,255,255,1);
	border-radius: 50%;
	overflow: hidden;
}

.card .overlay .img-box image{
	width: 100%;
	height: 100%;
}

.card .overlay .content {
	flex: 1;
	margin-left: 22rpx;
}

.card .overlay .name {
	height:40rpx;
	font-size:40rpx;
	font-weight:700;
	line-height:40rpx;
}

.card .overlay .name.black {
	color:#282828;
}

.card .overlay .name.default {
	color:#5C565D;
}

.card .overlay .name.vip {
	color:#FFDC29;
}

.card .overlay .time {
	height:26rpx;
	font-size:24rpx;
	line-height:26rpx;
	margin-top: 12rpx;
}

.card .overlay .time.default {
	color:#626262;
}

.card .overlay .time.vip {
	color:#FBFBFF;
}
.ul {
	margin: 64rpx;
	overflow: hidden;
}

.ul .li {
	width: 50%;
	overflow: hidden;
	margin-bottom: 80rpx;
	box-sizing: border-box;
	display: inline-block;
	text-align: left;
	vertical-align: top;
}

.ul .li:nth-child(2n) {
	padding-left: 80rpx;
}

.ul .li .key1{
	height:28rpx;
	font-size:28rpx;
	font-weight:500;
	color:rgba(92,86,93,1);
	line-height:28rpx;
	margin-bottom: 22rpx;
}

.ul .li .key2{
	font-size:22rpx;
	font-weight:400;
	color:rgba(146,146,146,1);
	line-height:22rpx;
	margin-bottom: 8rpx;
}

.ul .li text{
	font-size:60rpx;
	font-weight:bold;
	color:rgba(40,40,40,1);
	line-height:60rpx;
	display: inline-block;
	/*font-family: designFont;*/
}

.ul .li .key3{
	height:24rpx;
	font-size:24rpx;
	font-weight:400;
	color:rgba(146,146,146,1);
	line-height:24rpx;
}

.ul .li .chine {
	height:42rpx;
	font-size:42rpx;
	font-weight:900;
	color:rgba(40,40,40,1);
	line-height:66rpx;
	margin-bottom: 19rpx;
}

.contact {
	position: fixed;
	bottom: 0;
	width: 100%;
	bottom: 48rpx;
	left: 0;
	text-align: center;
}

.contact .tips {
	height:37rpx;
	font-size:26rpx;
	font-weight:400;
	color:rgba(92,86,93,1);
	line-height:37rpx;
	text-align: center;
}

.contact .phone {
	height:36rpx;
	font-size:40rpx;
	font-weight:bold;
	color:rgba(101,39,145,1);
	line-height:36rpx;
	margin-top: 18rpx;
}

.contact .icon-dianhua1 {
	color: #652791;
	vertical-align: top;
	display: inline-block;
	margin-right: 18rpx;
}

.expired-tips {
	height:120rpx;
	background:rgba(255,251,230,1);
	font-size:26rpx;
	color:rgba(255,127,76,1);
	line-height:40rpx;
	box-sizing: border-box;
	padding: 20rpx 40rpx;
}