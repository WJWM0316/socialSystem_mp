		<view class="list-box {{indexShowCount.advisor ? 'normal' : ''}}  {{collectMySelf.list.length === 0 ? '' : 'nodata'}}" hidden="{{pageList === 'browseMySelf'}}">
			<form bindsubmit="formSubmit" report-submit='{{true}}'>
				<button
					form-type="submit"
					bindtap="viewResumeDetail"
					class="formIdBtn"
					wx:for="{{collectMySelf.list}}"
					wx:for-item="item"
					data-jobhunterUid="{{item.uid}}"
					data-type="{{item.myType}}"
					wx:key="{{index}}">
					<view class="item2">
						<!-- 无发布职位和分享按钮 start -->
						<block wx:if="{{!item.myType}}">
							<view class="row-one">
								<view class="img-box"><image src="{{item.avatar.smallUrl}}" lazy-load></image></view>
								<view class="content">
									<view class="header">
										<view class="name">{{item.name}}</view>
										<view class="zhiwei">
											<block>{{item.workAgeDesc}}</block>
											<block wx:if="{{item.degreeDesc}}"> · {{item.degreeDesc}}</block>
											<block wx:if="{{item.salaryCeil}}"> · {{item.salaryFloor}}-{{item.salaryCeil}}k</block>
										</view>
									</view>
									<view class="degress" wx:if="{{item.lastCompanyName || item.lastPosition}}">
										<block wx:if="{{item.lastCompanyName}}">{{item.lastCompanyName}}</block>
										<block wx:if="{{item.lastCompanyName && item.lastPosition}}"> | </block>
										<block wx:if="{{item.lastPosition}}">{{item.lastPosition}}</block>
									</view>
								</view>
							</view>
							<!-- <view class="reddot"></view> -->
							<view class="row-two">
								<!-- 正常列表数据 start -->
								<block wx:if="{{!item.isRecommend}}">
									<view class="day">{{item.viewAt}}</view>
									<view class="action">
										{{item.gender === 2 ? '她' : '他'}}
										<i class="icon iconfont icon-biaojiweiganxingqu"></i>
										你的
									</view>
									<view class="position">
										<block wx:if="{{item.positionName}}">{{item.positionName}}</block>
										<block wx:if="{{!item.positionName}}">个人主页</block>
									</view>
								</block>
								<!-- 正常列表数据 end -->

								<!-- 系统或者官方推荐 start -->
								<block wx:else>
									<view class="featured-icon">热门</view>
									<view class="text">
										根据你的
										<view class="position limit-wid">{{item.positionName}}</view>
										为你推荐
									</view>
								</block>
								<!-- 系统或者官方推荐 end -->
							</view>
						</block>
						<!-- 无发布职位和分享按钮 end -->
						<block wx:else>
							<view class="public">
								<view class="row1">Get更多优质简历</view>
								<block wx:if="{{item.myType === 1}}">
									<view class="row2">根据职位为你匹配优质候选人</view>
									<view class="row3">发布/开放职位<i class="icon iconfont icon-disejiantou-you"></i></view>
								</block>
								<block wx:if="{{item.myType === 2}}">
									<view class="row2">努力就有回报，主动出击去转发吧~</view>
									<view class="row3">去转发主页<i class="icon iconfont icon-disejiantou-you"></i></view>
								</block>
								<image src="{{cdnImagePath}}image0001.png" class="image"></image>
							</view>
						</block>
					</view>
				</button>
				<view class="item2">
					<bottomStatus status="{{onBottomStatus}}" listData="{{collectMySelf.list}}"  desc="点击上方按钮分享，\r\n 一大波候选人等着开撩你！" paddingTop="{{collectMySelf.list.length === 0 ? '55' : ''}}" marginTop="{{0}}">
						<button class="jump share-position" bindtap="routeJump" data-route="position" wx:if="{{!detail.positionNum && !collectMySelf.list.length}}">发布/开放职位</button>
					</bottomStatus>
				</view>
			</form>
		</view>
		<view class="list-box {{browseMySelf.list.length === 0 ? '' : 'nodata'}}" hidden="{{pageList === 'collectMySelf'}}">
			<form bindsubmit="formSubmit" report-submit='{{true}}'> 
				<button
					form-type="submit"
					bindtap="viewResumeDetail"
					class="formIdBtn"
					wx:for="{{browseMySelf.list}}"
					wx:for-item="item"
					data-jobhunterUid="{{item.uid}}"
					data-type="clearRedDot"
					wx:key="{{index}}">
					<view class="item2">
						<view class="row-one">
							<view class="img-box"><image src="{{item.avatar.smallUrl}}" lazy-load></image></view>
							<view class="content">
								<view class="header">
									<view class="name">{{item.name}}</view>
									<view class="zhiwei">
										<block>{{item.workAgeDesc}}</block>
										<block wx:if="{{item.degreeDesc}}"> · {{item.degreeDesc}}</block>
										<block wx:if="{{item.salary}}"> · {{item.salary}}</block>
									</view>
								</view>
								<view class="degress" wx:if="{{item.lastCompanyName || item.lastPosition}}">
									<block wx:if="{{item.lastCompanyName}}">{{item.lastCompanyName}}</block>
									<block wx:if="{{item.lastCompanyName && item.lastPosition}}"> | </block>
									<block wx:if="{{item.lastPosition}}">{{item.lastPosition}}</block>
								</view>
							</view>
						</view>
						<view class="row-two">
							<view class="day">{{item.viewAt}}</view>
							<view class="action">
								查看了你的{{item.positionName ? '职位' : ''}}
							</view>
							<view class="position">
								<block wx:if="{{item.positionName}}">{{item.positionName}}</block>
								<block wx:if="{{!item.positionName}}">个人主页</block>
							</view>
						</view>
					</view>
				</button>
				<view class="item2">
					<bottomStatus status="{{onBottomStatus}}" listData="{{browseMySelf.list}}" desc="还没有候选人看过你哟～ \r\n试试分享你的职位，让更多人来围观吧~" paddingTop="{{browseMySelf.list.length === 0 ? '55' : ''}}" marginTop="{{0}}"></bottomStatus>
				</view>
			</form>
		</view>
	</view>

  <tabBar tabType="1" id="bottomRedDotBar"></tabBar>
  <choose wx:if="{{!choseType}}"></choose>
  <followWechat userInfo="{{userInfo}}"></followWechat>
	<shareBtn posterType="company" id="shareBtn" shareBtn="{{false}}"></shareBtn>
	<block wx:if="{{companyInfos.id}}"><canvasCard cardData="{{companyInfos}}" type="company" bindgetCreatedImg="getCreatedImg"></canvasCard></block>
