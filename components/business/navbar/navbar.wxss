@import '../../../iconfont.wxss'

.navbar {
  width: 100%;
  /*overflow: hidden;*/
  top: 0;
  left: 0;
  z-index: 10;
  position: fixed;
  transition: all 0.3s ease;
}


.navbar-title {
  width: 100%;
  height: 92rpx;
  line-height: 92rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  color: #22292C;
  font-size: 34rpx;
}

.return-back {
  position: absolute;
  left: 0;
  color: #22292C;
  top: 50%;
  transform: translateY(-50%);
  display: flex;
  align-items: center;
  height: 100%;
}
.formIdBox  {
  display: block;
  height: 100%;
}
.line {
  width:2rpx;
  height:36rpx;
  border-left:2rpx solid rgba(151,151,151,1);
  vertical-align:middle;
  display:inline-block;

}
.backLast {
  font-size: 34rpx;
}
.backHome {
  font-size: 39rpx;
  position: relative;
}
.formIdBtn {
  padding: 0;
  background: none;
  display:inline-block;
  line-height: 92rpx;
  overflow:visible;
}
.formIdBtn:after {
  display: none;
}

.page-title {
  max-width: 350rpx;
  display: inline-block;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  font-weight: 700;
  font-weight: 34rpx;
}

.showScanIcon {
  color: white;
  font-size: 35rpx;
  margin-left: 15rpx;
}

.showScanIconTips {
  width:425rpx;
  height:84rpx;
  position: absolute;
  left: 15rpx;
  top: 77rpx;
}

.btn {
  padding:0 22rpx 0 30rpx
}
.btn.btn1 {
  padding:0 22rpx 0 22rpx
}
.homeBubble {
  position: absolute;
  left:15rpx;
  top:77rpx;
  width: 367rpx;
  height: 86rpx;
}